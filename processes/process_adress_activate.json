{
   "labels":{
      "en":"Activate adress",
      "fr":"Activer addresse"
   },
   "type":{
      "code":"AD_ACT",
      "description":{
         "labels":{
            "fr":"Active une addresse",
            "en":"Activate adress"
         }
      }
   },
   "name":{
      "pattern":"%type\/%date\/%increment",
      "date_format":"Ym"
   },
   "border":false,
   "category":"adress_mgt",
   "available_in":[
      "page_adress"
   ],
   "context":{
      "function":"getParty",
      "entity":"adress"
   },
   "validationOnLoading":true,
   "return":"erase",
   "cancellation":{
      "state":"cancelled",
      "warning":{
         "title":{
            "labels":{
               "fr":"titre modal annuller",
               "en":"Ttitle modal"
            }
         },
         "message":{
            "labels":{
               "fr":"Voulez vous annuler ce process ?",
               "en":"Ttitle modal message"
            }
         },
         "button_close":{
            "labels":{
               "fr":"Fermer",
               "en":"Close"
            }
         },
         "button_ok":{
            "labels":{
               "fr":"Oui",
               "en":"OK"
            }
         }
      }
   },
   "communication":{
      "create":{
         "parameters":{
            "type":"toast",
            "class":"success",
            "dismissable":true,
            "duration":10000
         },
         "labels":{
            "fr":"<i class=\"bi bi-info-circle m-2\"></i> Process %process_name created",
            "en":"<i class=\"bi bi-info-circle m-2\"></i> Process %process_name created"
         }
      },
      "update":{
         "parameters":{
            "type":"alert",
            "class":"success",
            "dismissable":true,
            "duration":10000
         },
         "labels":{
            "fr":"<i class=\"bi bi-info-circle m-2\"></i> Process %process_name updated",
            "en":"<i class=\"bi bi-info-circle m-2\"></i> Process %process_name updated"
         }
      }
   },
   "rights":{
      "read":[
         "user"
      ],
      "edit":[
         "user",
         "admin"
      ]
   },
   "tasks":{
      "parameters":{
         "labels":{
            "en":"Tasks list",
            "fr":"Liste des tâches"
         }
      },
      "rights":{
         "read":[
         ],
         "edit":[
            "user",
            "super_admin",
            "admin"
         ]
      }
   },
   "breadCrumbs":{
      "display":true,
      "head":{
         "icon":"bi bi-geo-alt",
         "color":"rgb(245,246,247)",
         "bgcolor":"green"
      },
      "button":[
         
      ]
   },
   "lifecycle":{
      "done":{
         "labels":{
            "en":"Adress activation",
            "fr":"Activation addresse"
         },
         "bootstrap_class":"success",
         "tasks":[
            {
               "name":"activate_adress",
               "config_file":"task_adress_update.json"
            }
         ],
         "initial_values":{
            "state":"active"
         },
         "widgets":[
            {
               "name":"user_details",
               "type":"layout",
               "columns":[
                  {
                     "width":3,
                     "widgets":[
                        {
                           "type":"widget_taskDetails",
                           "parameters":{
                              "embeded":true,
                              "labels":{
                                 "fr":"Détail tâches",
                                 "en":"Tasks details"
                              }
                           }
                        },
                        {
                           "type":"widget_processInfo",
                           "parameters":{
                              "embeded":true
                           }
                        }
                     ]
                  },
                  {
                     "width":9,
                     "widgets":[
                        {
                           "type":"widget_alert",
                           "parameters":{
                              "labels":{
                                 "fr":"Client activé",
                                 "en":"Customer activtaed"
                              },
                              "type":"success",
                              "dismissable":true
                           }
                        },
                        {
                           "type":"widget_createdEntity",
                           "parameters":{
                              "entity":[
                                 
                              ]
                           }
                        }
                     ]
                  }
               ]
            }
         ],
         "button":[
            {
               "type":"refresh",
               "parameters":[
                  
               ],
               "rights":{
                  "read":[
                     
                  ]
               }
            }
         ],
         "transitions":[
            
         ],
         "confirmUnload":false
      }
   }
}