{
   "name":"contactPerson",
   "labels":{
      "en":"Contact Person",
      "fr":"Personne de contact"
   },
   "icon":"bi bi-person-rolodex",
   "parents":[
      "brand"
   ],
   "unicity":{
      "parent":true,
      "characteristics":[
         [
            "firstName",
            "lastName"
         ]
      ]
   },
   "db_table":"contactPerson",
   "searchable":true,
   "default_page":"page_contactPersonDetails",
   "default_process":"process_contactperson_new",
   "lineage_tree":{
      "title":"%firstName %lastName",
      "characteristics":[
         "state",
         "firstName",
         "lastName"
      ]
   },
   "characteristics":[
   {
         "name":"language",
         "type":"monoselect",
         "data_id":"language",
         "default":"en",
         "icon":"bi bi-translate",
         "labels":{
            "en":"language",
            "fr":"langue"
         },
         "value_type":"static",
         "value":[
            {
               "value":"fr",
               "labels":{
                  "en":"French",
                  "fr":"Français"
               }
            },
            {
               "value":"en",
               "labels":{
                  "en":"English",
                  "fr":"Anglais"
               }
            },
            {
               "value":"nl",
               "labels":{
                  "en":"Dutch",
                  "fr":"Néerlandais"
               }
            },
            {
               "value":"hu",
               "labels":{
                  "en":"Hungarian",
                  "fr":"Hongrois"
               }
            },
            {
               "value":"pl",
               "labels":{
                  "en":"Polish",
                  "fr":"Polonais"
               }
            },
            {
               "value":"de",
               "labels":{
                  "en":"German",
                  "fr":"Allemand"
               }
            }
         ],
         "mandatory":true,
         "listing":{
            "filter":{
               "indexed":true,
               "type":"radio"
            },
            "type":"flag"
         },
         "disabled":false,
         "rights":{
            "read":[
               
            ],
            "edit":[
               "super_admin",
               "admin",
               "user"
            ]
         }
      },
      {
         "type":"relation",
         "name":"adress",
         "data_id":"adress",
         "labels":{
            "en":"Adress",
            "fr":"Adresse"
         },
         "entity":"adress",
         "display_format":"%streetNumber %streetName <br>%zipCode %city<br><strong>%country</strong>"
      },
      {
         "name":"firstName",
         "type":"text",
         "data_id":"firstName",
         "mandatory":true,
         "labels":{
            "en":"First name",
            "fr":"Prénom(s)"
         },
         "disabled":false,
         "rights":{
            "read":[
               "super_admin"
            ],
            "edit":[
               "user",
               "admin",
               "super_admin"
            ]
         }
      },
      {
         "name":"id",
         "labels":{
            "en":"id",
            "fr":"id"
         }
      },
      {
         "name":"lastName",
         "type":"text",
         "data_id":"lastName",
         "mandatory":true,
         "labels":{
            "en":"last name",
            "fr":"Nom de famille"
         },
         "disabled":false,
         "validation":{
            "text-transform":"capitalize"
         },
         "rights":{
            "read":[
               "super_admin"
            ],
            "edit":[
               "user",
               "admin",
               "super_admin"
            ]
         }
      },
      {
         "name":"dob",
         "type":"date",
         "labels":{
            "fr":"date de naissance",
            "en":"date of birth"
         }
      },
      {
         "name":"parent",
         "labels":{
            "en":"Parent",
            "fr":"parent"
         },
         "display_format":"%firstName %lastName"
      },
      {
         "name":"hierarchy",
         "labels":{
            "en":"hierarcy",
            "fr":"hierarchy"
         },
         "display_format":"%firstName %lastName"
      },
	  {
         "name":"email",
         "type":"email",
         "data_id":"email",
         "icon":"fas fa-at",
         "labels":{
            "en":"Email",
            "fr":"Email"
         },
         "validation":{
            "pattern":"[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,4}$"
         },
         "mandatory":true
      },
      {
         "name":"state",
         "type":"monoselect",
         "data_id":"state",
         "icon":"bi bi-translate",
         "labels":{
            "en":"State",
            "fr":"Etat"
         },
         "value_type":"dynamic",
         "value":{
            "function_name":"getLifeCycleList",
            "function_param":"contactPerson"
         },
         "mandatory":true,
         "disabled":false,
         "listing":{
            "filter":{
               "indexed":true,
               "type":"radio"
            },
            "type":"state"
         },
         "rights":{
            "read":[
               
            ],
            "edit":[
               "super_admin"
            ]
         }
      }
   ],
   "lifecycle":{
      "prospect":{
         "labels":{
            "en":"Prospect",
            "fr":"Prospect"
         },
         "bootstrap_class":"purple",
         "transitions":[
            {
               "labels":{
                  "en":"Activate",
                  "fr":"Activer"
               },
               "process":"process_contactperson_activate",
               "target":"active"
            }
         ]
      },
      "active":{
         "labels":{
            "en":"Active",
            "fr":"Actif"
         },
         "bootstrap_class":"success",
         "transitions":[
            {
               "labels":{
                  "en":"Terminate",
                  "fr":"Supprimer"
               },
               "process":"process_contactperson_terminate",
               "target":"terminated"
            }
         ]
      },
      "terminated":{
         "labels":{
            "en":"terminated",
            "fr":"Supprimé"
         },
         "bootstrap_class":"danger",
         "transitions":[
            
         ],
         "access":{
            "auth":false,
            "labels":{
               "fr":"terminated welcome message",
               "en":"message terminé"
            }
         }
      }
   }
}