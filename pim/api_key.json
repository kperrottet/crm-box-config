{
   "name":"api_key",
   "labels":{
      "en":"API key",
      "fr":"Clef API"
   },
   "icon":"bi bi-key",
   "parents":[
      "brand"
   ],
   "unicity":[
      
   ],
   "db_table":"api_key",
   "default_page":"page_api_keyDetails",
   "default_process":"process_api_key_new",
   "lineage_tree":{
      "title":"%key",
      "characteristics":[
         "state"
      ]
   },
   "characteristics":[
      {
         "name":"key",
         "type":"text",
         "data_id":"key",
         "labels":{
            "en":"Key",
            "fr":"Clef"
         },
         "default":{
            "function":"random_password",
            "parameters":{
               "seed":true,
               "size":10
            }
         },
         "mandatory":true,
         "unicity":true,
         "rights":{
            "read":[
            ],
            "edit":[
               "user",
               "admin"
            ]
         }
      },
      {
         "name":"state",
         "type":"monoselect",
         "data_id":"state",
         "icon":"bi bi-translate",
         "labels":{
            "en":"State",
            "fr":"Etat"
         },
         "value_type":"dynamic",
         "value":{
            "function_name":"getLifeCycleList",
            "function_param":"api_key"
         },
         "mandatory":true,
         "disabled":false,
         "listing":{
            "filter":{
               "indexed":true,
               "type":"radio"
            },
            "type":"state"
         },
         "rights":{
            "read":[
               
            ],
            "edit":[
               "super_admin"
            ]
         }
      },
      {
         "name":"endPoints",
         "type":"multiselect",
         "data_id":"endPoints",
         "labels":{
            "en":"End Points",
            "fr":"End Points"
         },
         "value_type":"dynamic",
         "value":{
            "function_name":"getListEndPoints",
            "function_param":""
         },
         "mandatory":true,
         "rights":{
            "read":[
               
            ],
            "edit":[
               "super_admin"
            ]
         }
      }
   ],
   "lifecycle":{
      "active":{
         "labels":{
            "en":"Active",
            "fr":"Actif"
         },
         "bootstrap_class":"success",
         "transitions":[
            {
               "labels":{
                  "en":"Lock",
                  "fr":"Bloquer"
               },
               "process":"lock",
               "target":"locked"
            }
         ],
         "access":{
            "auth":true
         }
      },
      "locked":{
         "labels":{
            "en":"Locked",
            "fr":"Verrouillé"
         },
         "bootstrap_class":"warning",
         "transitions":[
            {
               "labels":{
                  "en":"Unlock",
                  "fr":"Déverrouiller"
               },
               "process":"unlock",
               "target":"active"
            },
            {
               "labels":{
                  "en":"Unlock",
                  "fr":"Déverrouiller"
               },
               "process":"unlock",
               "target":"terminated"
            }
         ]
      },
      "terminated":{
         "labels":{
            "en":"Terminated",
            "fr":"Supprimée"
         },
         "bootstrap_class":"danger",
         "transitions":[
         ]
      }
   }
}